<% for(var i=0; i<jfxx.length; i++) { %>
<div class="jf-list">
    <table width="100%" border="0" cellpadding="0" cellspacing="0">
        <tr>
            <td>缴费类型：<%= jfxx[i].pay_id %></td>
            <td>金额：<b class="fbred">¥<%= jfxx[i].plan_pay %></b></td>
            <% if(jfxx[i].pay_type==0) { %>
            <td rowspan="2"><a href="#" class="btn-yellow">缴费</a></td>
            <% } %>
            <% if(jfxx[i].pay_type!=0) { %>
            <td rowspan="2">
                <div class="btn-gray2">已缴费</div>
            </td>
            <% } %>
        </tr>
        <tr>
            <td colspan="2">缴费日期：<%= jfxx[i].start_date_str %>至<%= jfxx[i].end_date_str %></td>
        </tr>
    </table>
</div>
<% } %>


